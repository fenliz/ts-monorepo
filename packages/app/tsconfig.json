{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"target": "esnext",
		"useDefineForClassFields": true,
		"module": "esnext",
		"moduleResolution": "node",
		"jsx": "preserve",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"lib": [
			"esnext",
			"dom"
		]
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.tsx",
		"src/**/*.vue"
	]
}
